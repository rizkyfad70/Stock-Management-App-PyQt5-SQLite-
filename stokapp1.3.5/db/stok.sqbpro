table><table schema="main" name="stok_in" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_" freeze_columns="0"><sort/><column_widths><column index="1" value="35"/><column index="2" value="67"/><column index="3" value="79"/><column index="4" value="55"/><column index="5" value="56"/><column index="6" value="41"/><column index="7" value="87"/><column index="8" value="44"/><column index="9" value="47"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table><table schema="main" name="user_data" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_" freeze_columns="0"><sort/><column_widths><column index="1" value="55"/><column index="2" value="263"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 1*">CREATE TRIGGER update_balance_after_insert_stok_in
AFTER INSERT ON stok_in
FOR EACH ROW
BEGIN
    INSERT OR REPLACE INTO balance (part_name, part_number, mesin, kategori, lokasi, qty_blc)
    VALUES (NEW.part_name, NEW.part_number, NEW.mesin, NEW.kategori, NEW.lokasi, 
            COALESCE((SELECT qty_blc FROM balance WHERE part_name = NEW.part_name AND part_number = NEW.part_number AND mesin = NEW.mesin AND kategori = NEW.kategori AND lokasi = NEW.lokasi), 0) + NEW.qty_in);
END;</sql><sql name="SQL 2*">CREATE TRIGGER update_balance_after_insert_stok_out
AFTER INSERT ON stok_out
FOR EACH ROW
BEGIN
    INSERT OR REPLACE INTO balance (part_name, part_number, mesin, kategori, lokasi, qty_blc)
    VALUES (NEW.part_name, NEW.part_number, NEW.mesin, NEW.kategori, NEW.lokasi, 
            MAX(0, COALESCE((SELECT qty_blc FROM balance WHERE part_name = NEW.part_name AND part_number = NEW.part_number AND mesin = NEW.mesin AND kategori = NEW.kategori AND lokasi = NEW.lokasi), 0) - NEW.qty_out));
END;</sql><current_tab id="1"/></tab_sql></sqlb_project>
